{
  "name": "webapp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "description": "concise and scales well",
  "scripts": {
    "start": "NODE_ENV=development node webpack/webpack.develop.server.js",
    "build:web": "NODE_ENV=PRODUCTION webpack --config webpack/webpack.config.prod.js -p",
    "build:app": "NODE_ENV=PRODUCTION webpack --config webpack/webpack.config.render.js",
    "build": "npm run build:web && npm run build:app",
    "render": "NODE_ENV=production npm run build && node build/render.js",
    "server": "NODE_ENV=production pm2 start build/render.js",
    "reserver": "npm run stop && NODE_ENV=production pm2 start build/render.js",
    "stop": "pm2 stop build/render.js",
    "pm2:inspect": "pm2 show",
    "clear": "rm -r build && rm -r public"
  },
  "dependencies": {
    "babel-loader": "^6.2.5",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "font-awesome": "^4.6.3",
    "html-minifier": "^3.1.0",
    "log": "^1.4.0",
    "normalize.css": "^5.0.0",
    "pm2": "^2.0.18"
  },
