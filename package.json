    "immutable": "^3.8.1",
    "normalize.css": "^5.0.0",
    "pouchdb": "^6.1.0",
    "react": "15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^4.4.6",
    "react-router": "next",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
